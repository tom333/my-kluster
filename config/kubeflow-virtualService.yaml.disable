apiVersion: networking.istio.io/v1beta1
kind: VirtualService
metadata:
  name: kubeflow-route
  namespace: kubeflow
spec:
  hosts:
    - kubeflow.home.lan
  http:
    - name: "reviews-v2-routes"
      match:
        - uri:
            prefix: "/"
      route:
        - destination:
            host: istio-ingressgateway.svc.cluster.local
            port:
              number: 30721
