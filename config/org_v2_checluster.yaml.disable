apiVersion: org.eclipse.che/v2
kind: CheCluster
metadata:
  name: eclipse-che
  namespace: eclipse-che
spec:
  components: {}
  containerRegistry: {}
  devEnvironments:
      defaultNamespace:
        template: '<username>-che'
      storage:
        pvcStrategy: 'common'
  gitServices: {}
  networking:
    domain: 'che.tgu.ovh'
    tlsSecretName: 'che-secret'