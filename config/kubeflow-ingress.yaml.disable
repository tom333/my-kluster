apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  labels:
    app: kubeflow
  name: kubeflow-ingress
  namespace: istio-system
  annotations:
    kubernetes.io/ingress.class: "istio"
    cert-manager.io/issuer: "kubeflow-selfsigned-issuer"
spec:
  rules:
    - host: kubeflow2.home.lan
      http:
        paths:
          - path: /
            pathType: Prefix
            backend:
              service:
                name: istio-ingressgateway
                port:
                  number: 30721
  tls:
      - hosts:
        - ingress-demo.example.com
        secretName: kubeflow.home.lan
